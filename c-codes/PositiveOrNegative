WAP  to find whether a given number is positive or negative.
#include<stdio.h>
#include<conio.h>
void main()
{
int a;
clrscr();
printf(“Enter a number:”);
scanf(“%d”,&a);
if(a==0)
{
printf(“%d is neither positive nor negative”,a);
}
if (a>0)
{
printf(“%d is positive number”,a);
}
else
{
printf(“%d is negative number”,a);
}
getch();
}
