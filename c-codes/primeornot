WAP to find whether a given no is prime or not.
#include<stdio.h>
#include<conio.h>
void main()
{
int a,n,c=0,i;
clrscr();
printf(“Enter a number to be checked:”);
scanf(“%d”,&n);
for(i=1; i<=n; i++)
{
a=n%i;
if (a==0)
	{
	c=c+1;
	}
}
if (c==2)
{
printf(“%d is a prime no”,n);
}
else
{
printf(“%d is not prime no.”,n);
getch();
}
